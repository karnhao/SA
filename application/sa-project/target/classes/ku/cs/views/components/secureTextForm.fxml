<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" stylesheets="@../../themes/default.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ku.cs.controller.SecureTextFormController">
   <children>
      <Label fx:id="title" text="Title">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <HBox alignment="CENTER_LEFT" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
         <children>
            <VBox fx:id="vBox" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
               <children>
                  <TextField fx:id="textField" promptText="Prompt Text" styleClass="my-text-field">
                     <padding>
                        <Insets top="-2.0" />
                     </padding>
                  </TextField>
                  <PasswordField fx:id="passwordField" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" promptText="Prompt Text" styleClass="my-text-field">
                     <padding>
                        <Insets top="-30.0" />
                     </padding>
                     <VBox.margin>
                        <Insets />
                     </VBox.margin>
                  </PasswordField>
               </children>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </VBox>
            <ImageView fx:id="imageView" fitHeight="24.0" fitWidth="24.0" onMouseClicked="#onHiddenToggleClick" pickOnBounds="true" preserveRatio="true" HBox.hgrow="NEVER" />
         </children>
      </HBox>
      <Pane fx:id="pane" prefHeight="5.0" style="-fx-background-color: -dark-green-color;" stylesheets="@../../themes/default.css" />
   </children>
</VBox>
