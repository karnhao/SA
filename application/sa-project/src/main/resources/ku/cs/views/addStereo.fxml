<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ku.cs.controller.AddStereoController">
   <children>
      <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
         <children>
            <Label style="-fx-font-size: 36; -fx-font-weight: bold;" text="Stereo List" textFill="#006664">
               <padding>
                  <Insets left="10.0" />
               </padding></Label>
            <Pane HBox.hgrow="ALWAYS" />
            <TextField fx:id="searchStereoField" promptText="Search by type." style="-fx-background-radius: 20;">
               <HBox.margin>
                  <Insets left="10.0" right="10.0" top="17.0" />
               </HBox.margin></TextField>
            <Button mnemonicParsing="false" onAction="#onSearchStereo" styleClass="button-green" stylesheets="@../themes/default.css" text="Search" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="10.0" top="17.0" />
               </HBox.margin></Button>
         </children>
      </HBox>
      <HBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308">
         <children>
            <TableView fx:id="MusicianListTable" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" HBox.hgrow="ALWAYS">
               <columns>
                  <TableColumn fx:id="typeStereoColumn" prefWidth="75.0" text="Type" />
                  <TableColumn fx:id="nameOwnerStereoColumn" prefWidth="75.0" text="Name" />
                  <TableColumn fx:id="emailOwnerStereoColumn" prefWidth="75.0" text="Email" />
                  <TableColumn fx:id="phoneNumberOwnerColumn" prefWidth="75.0" text="Phone Number" />
               </columns>
               <columnResizePolicy>
                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy>
            </TableView>
         </children>
      </HBox>
      <HBox alignment="CENTER" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" VBox.vgrow="ALWAYS">
         <children>
            <Button mnemonicParsing="false" onAction="#addStereoToRequirement" style="-fx-font-size: 16; -fx-font-weight: bold;" styleClass="button-green" stylesheets="@../themes/default.css" text="Add" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets right="20.0" />
               </HBox.margin></Button>
            <Button mnemonicParsing="false" onAction="#backToEventRequirement" style="-fx-font-size: 16; -fx-font-weight: bold;" styleClass="button-green" stylesheets="@../themes/default.css" text="Back" HBox.hgrow="ALWAYS">
               <HBox.margin>
                  <Insets left="20.0" />
               </HBox.margin></Button>
         </children>
      </HBox>
   </children>
</VBox>
