<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ku.cs.controller.TextFormController">
   <children>
      <Label fx:id="title" text="Title">
         <font>
            <Font name="System Bold" size="24.0" />
         </font>
      </Label>
      <TextField fx:id="textField" promptText="Prompt Text" styleClass="my-text-field" stylesheets="@../../themes/default.css">
         <VBox.margin>
            <Insets />
         </VBox.margin>
         <padding>
            <Insets top="-2.0" />
         </padding></TextField>
      <Pane fx:id="pane" minHeight="5.0" prefHeight="5.0" style="-fx-background-color: -dark-green-color;" stylesheets="@../../themes/default.css" />
   </children>
</VBox>
